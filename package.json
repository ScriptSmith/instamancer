{
  "name": "instamancer",
  "version": "1.4.2",
  "description": "Scrape the Instagram API with Puppeteer",
  "main": "index.js",
  "types": "index.d.ts",
  "bin": {
    "instamancer": "src/cli.js"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "src/**/*.js",
    "src/**/*.d.ts"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "lint": "tslint -p tsconfig.json -p tests/tsconfig.json",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --write \"{src,tests}/**/*.ts\"",
    "clean": "rimraf src/**/*{.js,.d.ts} src/*{.js,.d.ts} index{.js,.d.ts}"
  },
  "author": "ScriptSmith",
  "license": "MIT",
  "keywords": [
    "instagram",
    "instagram api",
    "data mining",
    "scraping"
  ],
  "dependencies": {
    "await-lock": "^1.1.3",
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "concat-stream": "^2.0.0",
    "io-ts": "^2.0.0",
    "json2csv": "^4.5.1",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "pkgcloud": "^2.0.0",
    "puppeteer": "^1.17.0",
    "winston": "^3.2.1",
    "yargs": "^13.2.4"
  },
  "engines": {
    "node": ">=10.15.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ScriptSmith/instamancer.git"
  },
  "devDependencies": {
    "@types/concat-stream": "^1.6.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.13",
    "@types/json2csv": "^4.5.0",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^10.14.8",
    "@types/pkgcloud": "^1.7.1",
    "@types/puppeteer": "^1.12.4",
    "@types/request": "^2.48.1",
    "@types/yargs": "^12.0.12",
    "express": "^4.17.1",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.0.2",
    "prettier": "^1.18.2",
    "transform-json-types": "^0.7.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "preset": "ts-jest",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint"
    }
  },
  "prettier": {
    "trailingComma": "all",
    "arrowParens": "always",
    "bracketSpacing": false
  },
  "lint-staged": {
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "*.ts": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  }
}
